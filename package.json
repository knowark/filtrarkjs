{
  "name": "@knowark/filtrarkjs",
  "version": "0.3.4",
  "description": "Flexible Filtering for Javascript",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "**/lib/**"
    ]
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowark/filtrarkjs.git"
  },
  "keywords": [
    "polish",
    "notation",
    "domain",
    "filter",
    "comparison",
    "search"
  ],
  "author": "Knowark <it@knowark.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knowark/filtrarkjs/issues"
  },
  "homepage": "https://github.com/knowark/filtrarkjs#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3"
  }
}
